<% provide(:title, @user.name) %>
<div class="row">
  <aside class="span4">
    <section>
      <ul>
        <%= image_tag @user.avatar.url(:thumb) %>
        <li><%= @user.id %></li>
        <li><%= @user.name %></li>
        <li><%= @user.last_name %></li>
        <li><%= @user.email %></li>
        <li><%= @user.telnum %></li>
        <li><%= @user.municipality %></li>
      </ul>
    </section>
  </aside>
</div>