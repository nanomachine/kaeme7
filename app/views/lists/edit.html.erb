<% provide(:title, 'Edit List') %>
<div class="row-fluid sortable">
	<div class="box span12">
		<div class="box-header" data-original-title>
			<h2><i class="icon-edit"></i><span class="break"></span>Edit List</h2>
		</div>
		<div class="box-content">
	    	<%= form_for @list, :html => {:multipart => true, :class => "form-horizontal" }  do |f| %>
	    	<%= render 'list', f: f %>
			<% end %>
		</div>
	</div><!--/span-->
</div><!--/row-->